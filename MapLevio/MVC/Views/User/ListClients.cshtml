
@{
    ViewBag.Title = "Clients";
    Layout = "~/Views/Shared/_LayoutFront.cshtml";
}


<h2>ListUsers</h2>


@foreach (var p in ViewBag.result)
{


    string path = "~/Content/Upload/" + @p.image + ".jpg";



    <div class="emply-resume-list row mb-3">
        <div class="col-md-9 emply-info">
            <div class="emply-img">
                <img src="@Url.Content(path)" alt="" class="img-fluid">
            </div>
            <div class="emply-resume-info">
                <h4>
                    <a href="candidates_single.html">
                        @p.prenom
                    </a>
                </h4>
                <h5 class="mt-2"><span>Web Developer </span> at Company Name</h5>
                <p><i class="fas fa-map-marker-alt"></i>@p.ville / @p.pays</p>
                <ul class="links_bottom_emp mt-2">
                  <h3> 
                      <li><a href="candidates_single.html"><i class="far fa-envelope"></i> <span class="icon_text">@p.email</span></a></li>

                    </h3>
                    
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="col-md-3 emp_btn text-right">

          
          
                <i class="fas fa-eye">

                    @Html.ActionLink("Consulter Profile", "ClientSingle", "User", new { id = @p.userId }, new { @class = "fas fa-ey" })
                </i>
            
        </div>
    </div>


}



